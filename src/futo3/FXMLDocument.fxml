<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" fx:id="hello" prefHeight="121.0" prefWidth="246.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="futo3.FXMLDocumentController">
    <children>
      <Label fx:id="label" alignment="CENTER" contentDisplay="CENTER" layoutX="59.0" layoutY="14.0" prefHeight="24.0" prefWidth="128.0" text="Please enter a grid size:" textAlignment="CENTER" />
      <TextField fx:id="sizeInput" alignment="CENTER" layoutX="107.0" layoutY="48.0" prefHeight="25.0" prefWidth="31.0" />
      <Button fx:id="generateBtn" alignment="CENTER" layoutX="4.0" layoutY="92.0" mnemonicParsing="false" onAction="#generateGrid" prefHeight="25.0" prefWidth="109.0" text="Generate puzzle">
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
      </Button>
      <Button fx:id="loadBtn" alignment="CENTER" layoutX="133.0" layoutY="92.0" mnemonicParsing="false" onAction="#loadSave" text="Load saved game">
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
      </Button>
    </children>
</AnchorPane>
